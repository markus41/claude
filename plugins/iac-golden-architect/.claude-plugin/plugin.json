{
  "name": "iac-golden-architect",
  "version": "1.0.0",
  "description": "The Dredgen - Comprehensive Infrastructure as Code command center for multi-cloud Terraform, Harness CD, HashiCorp Vault, and Terraformer. Built for enterprise architects establishing SOC2-compliant infrastructure patterns.",
  "author": {
    "name": "Golden Armada",
    "email": "architect@goldenarmada.io"
  },
  "license": "MIT",
  "repository": "https://github.com/the-Lobbi/iac-golden-architect",
  "keywords": [
    "terraform",
    "terraformer",
    "harness",
    "vault",
    "infrastructure-as-code",
    "iac",
    "multi-cloud",
    "aws",
    "azure",
    "gcp",
    "soc2",
    "compliance"
  ],
  "categories": ["devops", "cloud", "security"],
  "provides": {
    "commands": 8,
    "agents": 4,
    "skills": 5,
    "hooks": 2
  },
  "discovery": {
    "commands": {
      "enabled": true,
      "directory": "../commands",
      "pattern": "*.md"
    },
    "agents": {
      "enabled": true,
      "directory": "../agents",
      "pattern": "*.md"
    },
    "skills": {
      "enabled": true,
      "directory": "../skills",
      "pattern": "**/SKILL.md"
    },
    "hooks": {
      "enabled": true,
      "directory": "../hooks",
      "pattern": "hooks.json"
    }
  }
}
