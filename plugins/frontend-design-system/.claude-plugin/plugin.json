{
  "name": "frontend-design-system",
  "version": "1.0.0",
  "description": "263+ design styles with multi-tenant Keycloak theming for AI-powered frontend development",
  "author": {
    "name": "Markus Ahling"
  },
  "license": "MIT",
  "repository": "https://github.com/markus41/claude/tree/main/frontend-design-system",
  "keywords": [
    "frontend",
    "design-system",
    "keycloak",
    "theming",
    "css",
    "tailwind",
    "ui-ux",
    "multi-tenant",
    "accessibility"
  ],
  "categories": ["frontend", "code-generation"],
  "provides": {
    "commands": 8,
    "agents": 6,
    "skills": 4,
    "hooks": 4
  },
  "discovery": {
    "commands": {
      "enabled": true,
      "directory": "../commands",
      "pattern": "*.md"
    },
    "agents": {
      "enabled": true,
      "directory": "../agents",
      "pattern": "*.md"
    },
    "skills": {
      "enabled": true,
      "directory": "../skills",
      "pattern": "**/SKILL.md"
    },
    "hooks": {
      "enabled": true,
      "directory": "../hooks",
      "pattern": "hooks.json"
    }
  }
}
