      "logicalName": "rosa_activity",
      "displayName": "Consulting Activity",
      "pluralName": "Consulting Activities",
      "description": "Meetings, calls, emails, and tasks related to consulting work",
      "ownership": "UserOwned",
      "primaryColumn": "rosa_subject",
      "columns": {
        "rosa_subject": {
          "type": "String",
          "maxLength": 500,
          "required": true,
          "displayName": "Subject"
