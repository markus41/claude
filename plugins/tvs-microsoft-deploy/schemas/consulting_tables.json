        "rosa_invoicedamount": {
          "type": "Currency",
          "required": false,
          "displayName": "Invoiced Amount",
          "precision": 2
        },
        "rosa_projectmanager": {
          "type": "Lookup",
          "required": false,
          "displayName": "Project Manager",
          "relatedTable": "systemuser"
        },
        "rosa_description": {
          "type": "Memo",
          "maxLength": 10000,
          "required": false,
          "displayName": "Description"
        }
