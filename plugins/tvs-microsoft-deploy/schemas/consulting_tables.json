      "description": "Consulting projects and engagement contracts",
      "ownership": "UserOwned",
      "primaryColumn": "rosa_name",
      "columns": {
        "rosa_name": {
          "type": "String",
          "maxLength": 300,
          "required": true,
          "displayName": "Engagement Name"
