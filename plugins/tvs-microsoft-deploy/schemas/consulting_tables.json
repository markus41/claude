            {
              "label": "Subject Matter Expert",
              "value": 100000003
            },
            {
              "label": "Stakeholder",
              "value": 100000004
            },
            {
              "label": "End User",
              "value": 100000005
            }
          ]
        },
        "rosa_isactive": {
          "type": "Boolean",
          "required": false,
          "displayName": "Active",
          "defaultValue": true
        }
      }
    },
