      "displayName": "Consulting Account",
      "pluralName": "Consulting Accounts",
      "description": "Client accounts for consulting engagements across Lobbi and Medicare",
      "ownership": "UserOwned",
      "primaryColumn": "rosa_name",
      "columns": {
        "rosa_name": {
          "type": "String",
          "maxLength": 200,
          "required": true,
          "displayName": "Account Name"
        },
        "rosa_entity": {
          "type": "Choice",
          "required": true,
          "displayName": "Owning Entity",
          "description": "Which consulting entity owns this account",
          "options": [
            {
              "label": "Lobbi Consulting",
              "value": 100000000
            },
            {
              "label": "Medicare Consulting",
              "value": 100000001
            }
          ]
        },
        "rosa_industry": {
          "type": "Choice",
          "required": false,
          "displayName": "Industry",
          "options": [
            {
              "label": "Insurance",
              "value": 100000000
            },
            {
              "label": "Healthcare",
              "value": 100000001
            },
            {
              "label": "Financial Services",
              "value": 100000002
            },
            {
              "label": "Government",
              "value": 100000003
            },
            {
              "label": "Education",
              "value": 100000004
            },
            {
              "label": "Technology",
              "value": 100000005
            },
            {
              "label": "Other",
              "value": 100000006
            }
          ]
        },
        "rosa_status": {
          "type": "Choice",
          "required": true,
          "displayName": "Status",
          "options": [
            {
              "label": "Prospect",
              "value": 100000000
            },
            {
              "label": "Qualified Lead",
              "value": 100000001
            },
            {
              "label": "Active Client",
              "value": 100000002
            },
            {
              "label": "Inactive",
              "value": 100000003
            },
            {
              "label": "Archived",
              "value": 100000004
            }
          ]
        },
        "rosa_revenue": {
          "type": "Currency",
          "required": false,
          "displayName": "Annual Revenue",
          "precision": 2
        },
        "rosa_website": {
          "type": "String",
          "maxLength": 500,
          "required": false,
          "displayName": "Website",
          "format": "url"
        },
        "rosa_address": {
          "type": "Memo",
          "maxLength": 1000,
          "required": false,
          "displayName": "Address"
        },
        "rosa_accountmanager": {
          "type": "Lookup",
          "required": false,
          "displayName": "Account Manager",
          "relatedTable": "systemuser"
        }
      },
      "relationships": [
        {
          "type": "OneToMany",
          "relatedTable": "rosa_consultingcontact",
          "lookupColumn": "rosa_accountid",
          "displayName": "Account Contacts"
        },
        {
          "type": "OneToMany",
          "relatedTable": "rosa_engagement",
          "lookupColumn": "rosa_accountid",
          "displayName": "Account Engagements"
        }
      ]
    },
    "Contacts": {
      "logicalName": "rosa_consultingcontact",
      "displayName": "Consulting Contact",
      "pluralName": "Consulting Contacts",
      "description": "Contacts at consulting client organizations",
      "ownership": "UserOwned",
      "primaryColumn": "rosa_fullname",
      "columns": {
        "rosa_firstname": {
          "type": "String",
          "maxLength": 100,
          "required": true,
          "displayName": "First Name"
        },
        "rosa_lastname": {
          "type": "String",
          "maxLength": 100,
          "required": true,
          "displayName": "Last Name"
        },
        "rosa_fullname": {
          "type": "String",
          "maxLength": 200,
          "required": false,
          "displayName": "Full Name",
          "computed": true,
          "formula": "rosa_firstname + ' ' + rosa_lastname"
        },
        "rosa_email": {
          "type": "String",
          "maxLength": 320,
          "required": true,
          "displayName": "Email",
          "format": "email"
        },
        "rosa_phone": {
          "type": "String",
          "maxLength": 50,
          "required": false,
          "displayName": "Phone",
          "format": "phone"
        },
        "rosa_accountid": {
          "type": "Lookup",
          "required": true,
          "displayName": "Account",
          "relatedTable": "rosa_consultingaccount"
        },
        "rosa_title": {
          "type": "String",
          "maxLength": 200,
          "required": false,
