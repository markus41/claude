{
  "$schema": "./schema/registry.schema.json",
  "version": "5.0.0",
  "lastUpdated": "2026-02-23",
  "description": "Central registry for Claude Code resources - agents, skills, commands, workflows, MCPs, tools, plugins",
  "metadata": {
    "totalAgents": 137,
    "totalSkills": 53,
    "totalCommands": 120,
    "totalWorkflows": 20,
    "totalMcpServers": 17,
    "totalMcpTools": 185,
    "totalBuiltinTools": 18,
    "totalPlugins": 20,
    "totalAvailablePlugins": 6,
    "totalTeams": 16,
    "lastConsolidation": "2026-02-23"
  },
  "indexes": {
    "agents": "./agents.minimal.json",
    "agentsFull": "./agents.index.json",
    "skills": "./skills.minimal.json",
    "skillsFull": "./skills.index.json",
    "commands": "./commands.minimal.json",
    "commandsFull": "./commands.index.json",
    "workflows": "./workflows.minimal.json",
    "workflowsFull": "./workflows.index.json",
    "mcps": "./mcps.minimal.json",
    "mcpsFull": "./mcps.index.json",
    "tools": "./tools.index.json",
    "plugins": "./plugins.index.json",
    "teams": "./teams.minimal.json",
    "teamsFull": "./teams.index.json"
  },
  "loadStrategy": {
    "default": "minimal",
    "onDemand": [
      "agentsFull",
      "skillsFull",
      "commandsFull",
      "workflowsFull",
      "mcpsFull",
      "teamsFull",
      "activation"
    ],
    "note": "Use minimal indexes at startup, load full metadata on-demand. Minimal files ~10KB vs Full ~120KB"
  },
  "activation": {
    "unified": "./activation/unified.activation.json",
    "skills": "./activation/skills.activation.json"
  },
  "search": {
    "enabled": true,
    "fuzzyMatch": true,
    "keywordIndex": "./search/keywords.json"
  },
  "orchestration": {
    "protocol": "4.0.0",
    "mandatoryPhases": [
      "explore",
      "plan",
      "code",
      "test",
      "fix",
      "document"
    ],
    "subAgentLimits": {
      "minimum": 3,
      "maximum": 13,
      "default": 5
    },
    "stateDirectory": "../orchestration/state",
    "hooksScript": "../hooks/orchestration-hooks.sh"
  },
