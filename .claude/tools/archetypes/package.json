{
  "name": "@claude/archetypes",
  "version": "1.0.0",
  "description": "Plugin archetype system for creating standardized plugin templates with pre-configured patterns and best practices",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "archetype": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.ts",
    "watch": "tsc --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "keywords": [
    "plugin",
    "archetype",
    "template",
    "scaffolding",
    "code-generation",
    "best-practices",
    "claude"
  ],
  "author": "Lobbi-Docs",
  "license": "MIT",
  "dependencies": {
    "handlebars": "^4.7.8",
    "nunjucks": "^3.2.4",
    "inquirer": "^9.2.12",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "commander": "^11.1.0",
    "glob": "^10.3.10",
    "yaml": "^2.3.4",
    "picocolors": "^1.0.0",
    "ora": "^8.0.1",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^24.7.0",
    "@types/inquirer": "^9.0.7",
    "@types/nunjucks": "^3.2.6",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "eslint": "^8.56.0",
    "vitest": "^1.0.4",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
